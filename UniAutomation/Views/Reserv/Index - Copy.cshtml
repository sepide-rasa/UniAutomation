        function PostForm(datas, url, id) {
            var sendInfo = datas;
            $('#Lock').show();
            $.ajax({
                type: "POST",
                url: url,
                datatype: "json",
                data: JSON3.stringify(sendInfo),
                contentType: 'application/json',
                success: function (data) {
                    var m = data;
                    windowAppend("body", "/_metro/error");
                    $("#message").html(m.data);
                    switch (m.state) {
                        case 0:
                            $("#error .wintitle").html("ذخیره موفق");
                            break;
                        case 1:
                            $("#error .wintitle").html("خطا");
                            break;
                    }
                    $('#Lock').hide();
                    windows('#Main', '/Reserv/index/' + '@ViewBag.State');
                },
                failure: function (data) {
                    alert(data.data);
                }
            });
        }

        function CancelPostForm(datas, url, id) {
            var sendInfo = datas;
            $('#Lock').show();
            $.ajax({
                type: "POST",
                url: url,
                datatype: "json",
                data: sendInfo,
                success: function (data) {
                    var m = data;
                    windowAppend("body", "/_metro/error");
                    $("#message").html(m.data);
                    switch (m.state) {
                        case 0:
                            $("#error .wintitle").html("ذخیره موفق");
                            $('#Etebar').html('اعتبار شما: ' + m.Etebar + ' ریال');
                            if (m.nobat == 1) {
                                $('#CbosS' + id).attr('style', 'display:block;');
                                $('#MsgS' + id).attr('style', 'display:none;');
                            }
                            if (m.nobat == 2){
                                $('#CbosN' + id).attr('style', 'display:block;');
                                $('#MsgN' + id).attr('style', 'display:none;');
                            }
                            else if (m.nobat == 3) {
                                $('#CbosSH' + id).attr('style', 'display:block;');
                                $('#MsgSH' + id).attr('style', 'display:none;');
                            }
                            break;
                        case 1:
                            $("#error .wintitle").html("خطا");
                            break;
                    }
                    $('#Lock').hide();
                },
                failure: function (data) {
                    alert(data.data);
                }
            });
        }    
