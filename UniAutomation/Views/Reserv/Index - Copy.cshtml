    @{
        UniAutomation.Models.UniAutomationEntities p = new UniAutomation.Models.UniAutomationEntities();
        var user = p.sp_GetCharge(Convert.ToInt32(Session["PersonId"])).FirstOrDefault();
        string ss="";
        if (user.Charge < 0)
        { ss = "(بدهکار)";}
        var nowDate = p.sp_GetDate().FirstOrDefault();
        if (ViewBag.State == 2)
        {
            nowDate.fldDateTime = nowDate.fldDateTime.AddDays(7);
        }
        if (ViewBag.State == 3)
        {
            nowDate.fldDateTime = nowDate.fldDateTime.AddDays(14);
        }
        var date = MyLib.Shamsi.ShamsiDayname(MyLib.Shamsi.Miladi2ShamsiString(nowDate.fldDateTime));        
        int back = 0, front = 0;
        switch (date)
        {
            case "شنبه":
                front = 7;
                break;
            case "یکشنبه":
                back = 1;
                front = 5;
                break;
            case "دوشنبه":
                back = 2;
                front = 4;
                break;
            case "سه شنبه":
                back = 3;
                front = 3;
                break;
            case "چهارشنبه":
                back = 4;
                front = 2;
                break;
            case "پنج شنبه":
                back = 5;
                front = 1;
                break;
            case "جمعه":
                back = 6;
                break;
        }
        System.Collections.ArrayList ar = new System.Collections.ArrayList();
        ar.Clear();
        for (int i = back; i >= 0; i--)
        {
            var d = MyLib.Shamsi.Miladi2ShamsiString(nowDate.fldDateTime.AddDays(-i));
            ar.Add(d);
        }
        //if (back == 0 || front == 0)
        //{
        //    ar.Add(MyLib.Shamsi.Miladi2ShamsiString(nowDate.fldDateTime));
        //}
        for (int i = 0; i <= front; i++)
        {
            var d = MyLib.Shamsi.Miladi2ShamsiString(nowDate.fldDateTime.AddDays(i + 1));
            ar.Add(d);
        }
    }
